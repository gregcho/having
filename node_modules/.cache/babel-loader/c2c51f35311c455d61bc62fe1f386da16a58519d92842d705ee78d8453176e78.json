xt-h5\">{{ formTitle }}</span>\r\n                </v-card-title>\r\n\r\n                <v-card-text>\r\n                  <v-container>\r\n                    <v-row>\r\n                      <v-col\r\n                        cols=\"12\"\r\n                        sm=\"6\"\r\n                        md=\"4\"\r\n                      >\r\n                        <v-text-field\r\n                          v-model=\"editedItem.NAME\"\r\n                          label=\"품목\"\r\n                        ></v-text-field>\r\n                      </v-col>\r\n                      <v-col\r\n                        cols=\"12\"\r\n                        sm=\"6\"\r\n                        md=\"4\"\r\n                      >\r\n                        <v-text-field\r\n                          v-model=\"editedItem.gijun_count\"\r\n                          label=\"기준수량\"\r\n                        ></v-text-field>\r\n                      </v-col>\r\n                      <v-col\r\n                        cols=\"12\"\r\n                        sm=\"6\"\r\n                        md=\"4\"\r\n                      >\r\n                        <v-text-field\r\n                          v-model=\"editedItem.count\"\r\n                          label=\"재고\"\r\n                        ></v-text-field>\r\n                      </v-col>\r\n                      <v-col\r\n                        cols=\"12\"\r\n                        sm=\"6\"\r\n                        md=\"4\"\r\n                      >\r\n                        <v-text-field\r\n                          v-model=\"editedItem.order_count\"\r\n                          label=\"발주수량\"\r\n                        ></v-text-field>\r\n                      </v-col>\r\n                    </v-row>\r\n                  </v-container>\r\n                </v-card-text>\r\n\r\n                <v-card-actions>\r\n                  <v-spacer></v-spacer>\r\n                  <v-btn\r\n                    color=\"blue darken-1\"\